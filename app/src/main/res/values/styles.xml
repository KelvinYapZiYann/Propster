<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="main_tab_layout" parent="Theme.AppCompat">
        <item name="android:textSize">@dimen/tab_layout_font_size</item>
    </style>

    <style name="user_profile_rounded_image">
        <item name="backgroundColor">@color/white</item>
        <item name="strokeColor">@color/disabled_grey</item>
        <item name="strokeWidth">1dp</item>
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="property_expenses_uploaded_file_image">
        <item name="backgroundColor">@color/white</item>
        <item name="strokeColor">@color/disabled_grey</item>
        <item name="strokeWidth">1dp</item>
    </style>

    <style name="property_tenant_user_icon" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/property_tenant_user_icon</item>
        <item name="android:minHeight">23dip</item>
        <item name="android:maxHeight">25dip</item>
    </style>

</resources>